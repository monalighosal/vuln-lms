{
  "name": "vulnerable-lms",
  "version": "1.0.0",
  "description": "Learning Management System with intentional vulnerabilities for security training",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:secure": "SECURE_MODE=true node server.js",
    "start:vulnerable": "SECURE_MODE=false node server.js",
    "switch:secure": "node switch-mode.js secure",
    "switch:vulnerable": "node switch-mode.js vulnerable"
  },
  "keywords": ["lms", "security", "training", "vulnerabilities"],
